{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@abstract-components/*": [
				"src/app/shared/abstract-components/*"
			],
			"@action-menu/*": [
				"src/app/shared/modules/action-menu/*"
			],
			"@app/*": [
				"src/app/*"
			],
			"@app-action-menu/*": [
				"src/app/shared/modules/app-action-menu/*"
			],
			"@app-navigator/*": [
				"src/app/app-navigator/*"
			],
			"@app-pagination-controls/*": [
				"src/app/shared/modules/app-pagination-controls/*"
			],
			"@app-validating-form-inputs/*": [
				"src/app/shared/modules/app-validating-form-inputs/*"
			],
			"@big-loading-spinner/*": [
				"src/app/shared/modules/big-loading-spinner/*"
			],
			"@clear-form-on-init-and-alert-on-destroy/*": [
				"src/app/shared/modules/clear-form-on-init-and-alert-on-destroy/*"
			],
			"@data-transport-processor/*": [
				"src/app/shared/services/data-transport-processor/*"
			],
			"@data-transport-result-check/*": [
				"src/app/shared/services/data-transport-processor/data-transport-result-interpreter/data-transport-result-check/*"
			],
			"@data-transport-result-interpreter/*": [
				"src/app/shared/services/data-transport-processor/data-transport-result-interpreter/*"
			],
			"@environments/*": [
				"src/environments/*"
			],
			"@guards/*": [
				"src/app/guards/*"
			],
			"@global-action-menu/*": [
				"src/app/shared/modules/global-action-menu/*"
			],
			"@hoverable-container/*": [
				"src/app/shared/modules/hoverable-container/*"
			],
			"@html-image/*": [
				"src/app/shared/modules/html-image/*"
			],
			"@interfaces/*": [
				"src/app/shared/interfaces/*"
			],
			"@image-action-menu/*": [
				"src/app/shared/modules/image-action-menu/*"
			],
			"@menu-choice/*": [
				"src/app/shared/modules/app-action-menu/dropdown-menu/menu-choice/*"
			],
			"@read-only-data/*": [
				"src/app/shared/data/read-only/*"
			],
			"@run-post-success-tasks/*": [
				"src/app/shared/services/data-transport-processor/data-transport-result-interpreter/run-post-success-tasks/*"
			],
			"@runtime-state-data/*": [
				"src/app/shared/data/runtime-state/*"
			],
			"@services/*": [
				"src/app/shared/services/*"
			],
			"@start-data-process-button/*": [
				"src/app/shared/modules/start-data-process-button/*"
			],
			"@start-data-process-list-item/*": [
				"src/app/shared/modules/start-data-process-list-item/*"
			],
			"@string-constants/*": [
				"src/app/shared/string-constants/*"
			],
			"@submit-form-button/*": [
				"src/app/shared/modules/submit-form-button/*"
			],
			"@thumbnail-image-container/*": [
				"src/app/shared/modules/thumbnail-image-container/*"
			],
			"@tiny-loading-spinner/*": [
				"src/app/shared/modules/tiny-loading-spinner/*"
			],
			"@toggle-setting-in-browser-storage/*": [
			"src/app/shared/services/item-in-browser-storage/toggle-setting-in-browser-storage/*"
			],
			"@validating-input/*": [
				"src/app/shared/services/validating-input/*"
			],
			"@validating-inputs/*": [
				"src/app/shared/services/validating-inputs/*"
			],
			"@zoomable-html-image/*": [
				"src/app/shared/modules/zoomable-html-image/*"
			]
		},
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		]
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true
	}
}
